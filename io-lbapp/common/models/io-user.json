{
  "name": "ioUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    }
  },
  "properties": {
    "active": {
      "type": "boolean",
      "required": false,
      "default": true
    },
    "uuid": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "lastConnection": {
      "type": "date",
      "required": false,
      "default": null
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "userExists"
    }
  ],
  "methods": {}
}
